do(
  :=(func, import("modules/func.egg")),

  dict(
    "sum", :=(sum, fun(arr,
      get(func, "reduce")(arr, +, 0)
    )),

    "avg", :=(avg, fun(arr, do(
      /(sum(arr), length(arr))
    ))),

    "is_even", :=(is_even, fun(x,
      ==(%(x, 2), 0)
    )),

    "is_odd", :=(is_odd, fun(x,
      if(is_even(x), false, true)
    ))
  )
)
